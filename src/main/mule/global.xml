    <http:listener-config name="HTTP_Listener_config" doc:name="HTTP Listener config" doc:id="5fc4c544-f374-494e-94ec-4d08dee7cccd" basePath="covid">
		<http:listener-connection host="0.0.0.0" port="${http.port}" />
